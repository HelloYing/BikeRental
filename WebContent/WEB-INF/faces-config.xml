<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>BikeBean</managed-bean-name>
  <managed-bean-class>edu.osu.BikeBean</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>loginBean</managed-bean-name>
  <managed-bean-class>edu.osu.LoginBean</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>WelcomeBean</managed-bean-name>
  <managed-bean-class>edu.osu.WelcomeBean</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>registerBean</managed-bean-name>
  <managed-bean-class>edu.osu.RegisterBean</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>usermainBean</managed-bean-name>
  <managed-bean-class>edu.osu.UsermainBean</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
  <managed-property>
   <property-name>logBean</property-name>
   <property-class>edu.osu.LoginBean</property-class>
   <value>#{loginBean}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>availablebikeBean</managed-bean-name>
  <managed-bean-class>edu.osu.AvailableBikeBean</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
  <managed-property>
   <property-name>logBean</property-name>
   <property-class>edu.osu.LoginBean</property-class>
   <value>#{loginBean}</value>
  </managed-property>
  <managed-property>
   <property-name>usermainBean</property-name>
   <property-class>edu.osu.UsermainBean</property-class>
   <value>#{usermainBean}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>profileBean</managed-bean-name>
  <managed-bean-class>edu.osu.ProfileBean</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
  <managed-property>
   <property-name>logBean</property-name>
   <property-class>edu.osu.LoginBean</property-class>
   <value>#{loginBean}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>EventBean</managed-bean-name>
  <managed-bean-class>edu.osu.EventBean</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>RentBean</managed-bean-name>
  <managed-bean-class>edu.osu.RentBean</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 
 
 <navigation-rule>
  <display-name>login.xhtml</display-name>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>1</from-outcome>
   <to-view-id>/usermain.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>login.xhtml</display-name>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>2</from-outcome>
   <to-view-id>/staffMain.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 
  <navigation-rule>
  <display-name>login.xhtml</display-name>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>register</from-outcome>
   <to-view-id>/register.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>register.xhtml</display-name>
  <from-view-id>/register.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>login</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 
 
 <navigation-rule>
  <display-name>usermain.xhtml</display-name>
  <from-view-id>/usermain.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>profile</from-outcome>
   <to-view-id>/profile.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>orders</from-outcome>
   <to-view-id>/orderHistory.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>rentBike</from-outcome>
   <to-view-id>/availablebikes.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 
 
 <navigation-rule>
  <display-name>profile.xhtml</display-name>
  <from-view-id>/profile.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>back</from-outcome>
   <to-view-id>/usermain.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>

 <navigation-rule>
  <display-name>orderHistory.xhtml</display-name>
  <from-view-id>/orderHistory.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>back</from-outcome>
   <to-view-id>/usermain.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 
 
 <navigation-rule>
  <display-name>availablebikes.xhtml</display-name>
  <from-view-id>/availablebikes.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>back</from-outcome>
   <to-view-id>/usermain.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 

 <navigation-rule>
  <display-name>staffMain.xhtml</display-name>
  <from-view-id>/staffMain.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>editBike</from-outcome>
   <to-view-id>/staffBike.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>addBike</from-outcome>
   <to-view-id>/addBike.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>postEvent</from-outcome>
   <to-view-id>/postEvent.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>editEvent</from-outcome>
   <to-view-id>/updateEvent.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>updateRent</from-outcome>
   <to-view-id>/staffUpdateRent.xhtml</to-view-id>
  </navigation-case>
 <navigation-case>
   <from-outcome>logout</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 
 <navigation-rule>
  <display-name>updateRent.xhtml</display-name>
  <from-view-id>/staffUpdateRent.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>back</from-outcome>
   <to-view-id>/staffMain.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>updateEvent.xhtml</display-name>
  <from-view-id>/updateEvent.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>back</from-outcome>
   <to-view-id>/staffMain.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>staffBike.xhtml</display-name>
  <from-view-id>/staffBike.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>back</from-outcome>
   <to-view-id>/staffMain.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>addBike.xhtml</display-name>
  <from-view-id>/addBike.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>back</from-outcome>
   <to-view-id>/staffMain.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>postEvent.xhtml</display-name>
  <from-view-id>/postEvent.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>back</from-outcome>
   <to-view-id>/staffMain.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
